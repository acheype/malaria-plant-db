<?xml version="1.0" encoding="utf-8"?>
<workbook>
    <worksheet name="1 - PUBLI">
        <section startRow="0" endRow="0">
        </section>
        <loop startRow="1" endRow="1" items="PublicationLine" var="pub"
              varType="nc.ird.malariaplantdb.service.xls.dto.PublicationLine">
            <section startRow="1" endRow="1">
                <mapping row="1" col="0">pub.entryType</mapping>
                <mapping row="1" col="1">pub.authors</mapping>
                <mapping row="1" col="2" nullAllowed="true">pub.year</mapping>
                <mapping row="1" col="3">pub.title</mapping>
                <mapping row="1" col="4">pub.journal</mapping>
                <mapping row="1" col="5">pub.pages</mapping>
                <mapping row="1" col="6">pub.volume</mapping>
                <mapping row="1" col="7">pub.nbOfVolumes</mapping>
                <mapping row="1" col="8">pub.number</mapping>
                <mapping row="1" col="9">pub.bookTitle</mapping>
                <mapping row="1" col="10">pub.publisher</mapping>
                <mapping row="1" col="11">pub.edition</mapping>
                <mapping row="1" col="12">pub.conferenceName</mapping>
                <mapping row="1" col="13">pub.conferencePlace</mapping>
                <mapping row="1" col="14">pub.university</mapping>
                <mapping row="1" col="15">pub.institution</mapping>
                <mapping row="1" col="16">pub.doi</mapping>
                <mapping row="1" col="17">pub.pmid</mapping>
                <mapping row="1" col="18">pub.isbn</mapping>
                <mapping row="1" col="19">pub.url</mapping>
                <mapping row="1" col="20">pub.compilers</mapping>
                <mapping row="1" col="21">pub.compilersNotes</mapping>
            </section>
            <loopbreakcondition>
                <rowcheck offset="0">
                    <cellcheck offset="0"/>
                    <cellcheck offset="1"/>
                    <cellcheck offset="2"/>
                    <cellcheck offset="3"/>
                </rowcheck>
            </loopbreakcondition>
        </loop>
    </worksheet>
    <worksheet name="2 - SPECIES">
        <section startRow="0" endRow="0">
        </section>
        <loop startRow="1" endRow="1" items="SpeciesLine" var="species"
              varType="nc.ird.malariaplantdb.service.xls.dto.SpeciesLine">
            <section startRow="1" endRow="1">
                <mapping row="1" col="0">species.publication</mapping>
                <mapping row="1" col="1">species.family</mapping>
                <mapping row="1" col="2">species.species</mapping>
                <mapping row="1" col="3">species.speciesNameInPub</mapping>
                <mapping row="1" col="4" nullAllowed="true">species.isHerbariumVoucher</mapping>
                <mapping row="1" col="5">species.herbarium</mapping>
                <mapping row="1" col="6">species.country</mapping>
                <mapping row="1" col="7">species.continent</mapping>
            </section>
            <loopbreakcondition>
                <rowcheck offset="0">
                    <cellcheck offset="0"/>
                    <cellcheck offset="1"/>
                    <cellcheck offset="2"/>
                    <cellcheck offset="3"/>
                    <cellcheck offset="4"/>
                </rowcheck>
            </loopbreakcondition>
        </loop>
    </worksheet>
    <worksheet name="3 - PLANT INGREDIENTS">
        <section startRow="0" endRow="0">
        </section>
        <loop startRow="1" endRow="1" items="PlantIngredientsLine" var="plantIngredients"
              varType="nc.ird.malariaplantdb.service.xls.dto.PlantIngredientsLine">
            <section startRow="1" endRow="1">
                <mapping row="1" col="0">plantIngredients.publication</mapping>
                <mapping row="1" col="1">plantIngredients.species1</mapping>
                <mapping row="1" col="2">plantIngredients.partUsed1</mapping>
                <mapping row="1" col="3">plantIngredients.species2</mapping>
                <mapping row="1" col="4">plantIngredients.partUsed2</mapping>
                <mapping row="1" col="5">plantIngredients.species3</mapping>
                <mapping row="1" col="6">plantIngredients.partUsed3</mapping>
                <mapping row="1" col="7">plantIngredients.species4</mapping>
                <mapping row="1" col="8">plantIngredients.partUsed4</mapping>
                <mapping row="1" col="9">plantIngredients.species5</mapping>
                <mapping row="1" col="10">plantIngredients.partUsed5</mapping>
                <mapping row="1" col="11">plantIngredients.species6</mapping>
                <mapping row="1" col="12">plantIngredients.partUsed6</mapping>
                <mapping row="1" col="13">plantIngredients.species7</mapping>
                <mapping row="1" col="14">plantIngredients.partUsed7</mapping>
                <mapping row="1" col="15">plantIngredients.species8</mapping>
                <mapping row="1" col="16">plantIngredients.partUsed8</mapping>
                <mapping row="1" col="17">plantIngredients.species9</mapping>
                <mapping row="1" col="18">plantIngredients.partUsed9</mapping>
                <mapping row="1" col="19">plantIngredients.species10</mapping>
                <mapping row="1" col="20">plantIngredients.partUsed10</mapping>
            </section>
            <loopbreakcondition>
                <rowcheck offset="0">
                    <cellcheck offset="0"/>
                    <cellcheck offset="1"/>
                    <cellcheck offset="2"/>
                </rowcheck>
            </loopbreakcondition>
        </loop>
    </worksheet>
