<?xml version="1.0" encoding="utf-8"?>
<workbook>
    <worksheet name="1 - PUBLI">
        <section startRow="0" endRow="0">
        </section>
        <loop startRow="1" endRow="1" items="PublicationSheet" var="pub"
              varType="nc.ird.malariaplantdb.service.xls.dto.PublicationSheet">
            <section startRow="1" endRow="1">
                <mapping row="1" col="0">pub.entryType</mapping>
                <mapping row="1" col="1">pub.authors</mapping>
                <mapping row="1" col="2" nullAllowed="true">pub.year</mapping>
                <mapping row="1" col="3">pub.title</mapping>
                <mapping row="1" col="4">pub.journal</mapping>
                <mapping row="1" col="5">pub.month</mapping>
                <mapping row="1" col="6">pub.page</mapping>
                <mapping row="1" col="7">pub.volume</mapping>
                <mapping row="1" col="8">pub.nbOfVolumes</mapping>
                <mapping row="1" col="9">pub.number</mapping>
                <mapping row="1" col="10">pub.bookTitle</mapping>
                <mapping row="1" col="11">pub.publisher</mapping>
                <mapping row="1" col="12">pub.edition</mapping>
                <mapping row="1" col="13">pub.university</mapping>
                <mapping row="1" col="14">pub.institution</mapping>
                <mapping row="1" col="15">pub.doi</mapping>
                <mapping row="1" col="16">pub.pmid</mapping>
                <mapping row="1" col="17">pub.isbn</mapping>
                <mapping row="1" col="18">pub.url</mapping>
                <mapping row="1" col="19" nullAllowed="true">pub.isReviewed</mapping>
                <mapping row="1" col="20">pub.compilers</mapping>
                <mapping row="1" col="21">pub.compilersNotes</mapping>
            </section>
            <loopbreakcondition>
                <rowcheck offset="0">
                    <cellcheck offset="0"/>
                    <cellcheck offset="1"/>
                </rowcheck>
            </loopbreakcondition>
        </loop>
    </worksheet>
</workbook>